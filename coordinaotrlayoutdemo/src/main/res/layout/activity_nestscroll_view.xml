<?xml version="1.0" encoding="utf-8"?>
<com.example.coordinaotrlayoutdemo.nestscroll.NestedParentLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
 >
    <TextView
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:background="@color/colorAccent" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:background="@color/colorPrimaryDark" />

    <com.example.coordinaotrlayoutdemo.nestscroll.NestedListView
        android:id="@+id/list_view"
        android:layout_width="match_parent"
        android:layout_height="900dp"
        />
</com.example.coordinaotrlayoutdemo.nestscroll.NestedParentLinearLayout>